#include<iostream>
using namespace std;
int main()
{
	bool arr[100] = { false };
	int l;
	for (int student = 1; student <=100; student++)
	{
		for ( l = student - 1; l < 100; l+=student)
		{
			arr[l] = !arr[l];
		}
	}
	cout << "开着的储物柜编号为： ";
	for (int i = 0; i < 100; i++)
	{
		if (arr[i])
			cout << (i + 1) << " ";
	}
	cout << endl;
	return 0;
}
