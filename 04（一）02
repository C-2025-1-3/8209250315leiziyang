#include<iostream>
using namespace std;
void BubbleSort(double arr[],int n)
{
		for (int i = 0; i < n-1; i++)
		{
			for (int j = 0; j < n-1 - i; j++)
			{
				if (arr[j] > arr[j + 1])
				{
					double temp = arr[j + 1];
					arr[j + 1] = arr[j];
					arr[j] = temp;
				}
			}
		}
}
int main()
{
	double arr[10];
	cout << "Enter the numbers: ";
	for (int i = 0; i < 10; i++)
		cin >> arr[i];
	BubbleSort(arr,10);
	for (int i = 0; i < 10; i++)
		cout << arr[i] << " ";
	cout << endl;
	return 0;
}
